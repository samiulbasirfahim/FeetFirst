{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "paths": {
            "@/*": [
                "./*"
            ]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "declarations.d.ts",
        "expo-env.d.ts",
        "nativewind-env.d.ts"
    ]
}
